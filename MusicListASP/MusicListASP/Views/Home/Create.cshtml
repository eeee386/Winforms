@model MusicListASP.Models.Music
    <h1>Zene Hozzáadása</h1>
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div class="form-group">
                    <label asp-for="Title" class="control-label">Cím</label>
                    <input asp-for="Title" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="Artist" class="control-label">Előadó</label>
                    <input asp-for="Artist" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="PublishYear" class="control-label">Kiadás éve</label>
                    <input asp-for="PublishYear" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="MusicLength" class="control-label">Hossz</label>
                    <input asp-for="MusicLength" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="Priority" class="control-label">Prioritás</label>
                    <input asp-for="Priority" class="form-control" />
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>

        </div>
    </div>
    <div>
        <a asp-action="Index">Back to list</a>
    </div>
